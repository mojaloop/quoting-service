              "batchSize": 1,
              "pollFrequency": 10,
              "recursiveTimeout": 100,
              "messageCharset": "utf8",
              "messageAsJSON": true,
              "sync": true,
              "consumeTimeout": 1000
            },
            "rdkafkaConf": {
              "client.id": "quotes-handler-post_c",
              "group.id": "group-quotes-handler-post",
              "metadata.broker.list": "localhost:9092",
              "socket.keepalive.enable": true,
              "allow.auto.create.topics": true
